{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DeaDevboxWriteSSL",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:GetObject"
            ],
            "Resource": [
                "arn:aws:s3:::dea-devbox-config/SSL/*"
            ]
        },
        {
            "Sid": "DeaDevboxListSSL",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetBucketLocation"
            ],
            "Resource": [
                "arn:aws:s3:::dea-devbox-config"
            ]
        }
    ]
}
